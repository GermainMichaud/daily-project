{
  "name": "daily-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet --fix"
